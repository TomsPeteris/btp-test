{
	"authenticationMethod": "none",
	"routes": [
		{
			"source": "^/space/(.*)$",
			"target": "$1",
			"destination": "destination_space"
		},
		{
			"source": "^/subaccount/(.*)$",
			"target": "$1",
			"destination": "destination_subaccount"
		},
		{
			"source": "^/region/(.*)$",
			"target": "$1",
			"destination": "destination_region"
		}
	]
}
